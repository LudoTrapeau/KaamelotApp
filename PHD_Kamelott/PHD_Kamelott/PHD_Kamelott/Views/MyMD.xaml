<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PHD_Kamelott.Views.MyMD"
             xmlns:pages="clr-namespace:PHD_Kamelott.Views"
                  xmlns:dkMainPage="clr-namespace:PHD_Kamelott"
                  >
    
  <MasterDetailPage.Master>
    <pages:MyMDMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
                <dkMainPage:MainPage/>
            </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>